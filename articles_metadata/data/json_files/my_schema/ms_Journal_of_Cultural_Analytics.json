[{"url": "http://dx.doi.org/10.22148/16.008", "identifier": {"string_id": "10.22148/16.008", "id_scheme": "DOI"}, "abstract": "<meta name=\"citation_abstract\" content=\"Genre is a word whose time has come \u2014 and gone \u2014 and might now, perhaps, be coming back again. Debates about particular literary kinds have been com-mon in literary criticism since Aristotle\u2019s Poetics, but they acquired a new intensity and reflexivity in the third quarter of the twentieth century, as structuralists and poststructuralists struggled to redefine the concept of genre itself.\">", "article_title": "Genre Theory and Historicism", "authors": [{"given": "Ted", "family": "Underwood", "sequence": "first", "affiliation": []}, {"name": "The NovelTM Research Group", "sequence": "first", "affiliation": []}], "publisher": "CA: Journal of Cultural Analytics", "date": "2017-02-10T19:47:44Z", "keywords": [], "journal_title": "Journal of Cultural Analytics", "volume": "", "issue": "", "ISSN": [{"value": "2371-4549", "type": "electronic"}]}, {"url": "http://dx.doi.org/10.22148/16.011", "identifier": {"string_id": "10.22148/16.011", "id_scheme": "DOI"}, "abstract": "<meta name=\"citation_abstract\" content=\"The distinction between fiction and non-fiction, between a text that is true and one that is not, is one of the oldest on record. Ever since we have been thinking about the act of narration, we have addressed the related meanings of truth and imagination.\">", "article_title": "Fictionality", "authors": [{"given": "Andrew", "family": "Piper", "sequence": "first", "affiliation": []}], "publisher": "CA: Journal of Cultural Analytics", "date": "2017-02-10T19:47:44Z", "keywords": [], "journal_title": "Journal of Cultural Analytics", "volume": "", "issue": "", "ISSN": [{"value": "2371-4549", "type": "electronic"}]}, {"url": "http://dx.doi.org/10.22148/16.022", "identifier": {"string_id": "10.22148/16.022", "id_scheme": "DOI"}, "abstract": "<meta name=\"citation_abstract\" content=\"Literary  readings  provoke  strong  feelings,  which  feed  intense  critical  de-bates. And while recorded literary readings have long been available for study, few scholars have applied to them the empirical methods that the digitalhumanities and interdisciplinary sound studies now offer.\">", "article_title": "Beyond Poet Voice: Sampling the (Non-) Performance Styles of 100 American Poets", "authors": [{"given": "Marit", "family": "MacArthur", "sequence": "first", "affiliation": []}, {"given": "Georgia", "family": "Zellou", "sequence": "additional", "affiliation": []}, {"given": "Lee", "family": "Miller", "sequence": "additional", "affiliation": []}], "publisher": "CA: Journal of Cultural Analytics", "date": "2018-04-18T20:18:26Z", "keywords": [], "journal_title": "Journal of Cultural Analytics", "volume": "", "issue": "", "ISSN": []}, {"url": "http://dx.doi.org/10.22148/16.014", "identifier": {"string_id": "10.22148/16.014", "id_scheme": "DOI"}, "abstract": "<meta name=\"citation_abstract\" content=\"According to Rita Felski, context is overrated. Even in the sophisticated variants of contextualization typical of the New Historicism, she explains, scholars\u2019 obsession with historical context as the ultimate source of textual meaning disregards the capacity of literature to resonate across time and space.\">", "article_title": "Topic Modeling, Epistemology, and the English and German Novel", "authors": [{"given": "Matt", "family": "Erlin", "sequence": "first", "affiliation": []}], "publisher": "CA: Journal of Cultural Analytics", "date": "2017-05-08T16:43:15Z", "keywords": [], "journal_title": "Journal of Cultural Analytics", "volume": "", "issue": "", "ISSN": []}, {"url": "http://dx.doi.org/10.22148/16.010", "identifier": {"string_id": "10.22148/16.010", "id_scheme": "DOI"}, "abstract": "<meta name=\"citation_firstpage\" content=\"11066\">", "article_title": "Understanding Gender and Character Agency in the 19th Century Novel", "authors": [{"given": "Matthew", "fa